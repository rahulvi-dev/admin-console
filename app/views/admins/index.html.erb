<%= render partial: 'admin/dashboard/home' %>
<div class="col-lg-8 main">     
    <div class="row">
      <ol class="breadcrumb">
        <li><a href="/admin/home"><span class="glyphicon glyphicon-home" aria-hidden="true"></span></a></li>
        <li class="active"><%= @admins.table_name %></li>
      </ol>
    </div>
    <div class="row">
      <div class="col-lg-12">
        <h1 class="page-header"><%= @admins.table_name %></h1>
        <table class="table table-hover">
      </div>
    </div>
    <thead>
    <tr>
      <th>ID</th>
      <th>Email</th>
      <th>First Name </th>
      <th>Last Name </th>
      <th>City</th>
      <th>Mobile No</th>
      <th>Actions</th>
    </tr>
  </thead>
    <tbody>
    <% @admins.each do |admin| %>
      <tr>
        <td>
          <%= link_to admin.id, show_admin_path(admin.id) %>
        </td>
        <td>
          <%= admin.email %>
        </td>
        <td>
          <%= admin.first_name  %>
        </td>
        <td>
          <%= admin.last_name %>
        </td>
        <td>
          <%= admin.city %>
        </td>
        <td>
          <%= admin.mobile_number %>
        </td>
        <td>
          <%= link_to show_admin_path(admin.id) do %>
            <span class="glyphicon glyphicon-eye-open"></span>
          <% end %>
          |
          <%= link_to edit_admin_path(admin.id) do %>
            <span class="glyphicon glyphicon-pencil"></span>
          <% end %>
          |
          <%= link_to destroy_admin_path(admin.id), method: :delete, data: {confirm: "Are you Sure ?"}  do %>
            <span class="glyphicon glyphicon-trash"></span>
          <% end %>
        </td>
      </tr>
    <% end %>
  </tbody>
</table>
<strong>
  <%= will_paginate @admins   %>
</strong>
      </div>
    </div>
 </div><!--/.row-->
        